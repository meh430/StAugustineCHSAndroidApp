    xmlns:android="http://schemas.android.com/apk/res/android"
    android:opacity="opaque">

    <!-- BACKGROUND -->
    <item android:drawable="@android:color/white"/>

    <!-- LOGO -->
    <item
        android:left="200dp"
        android:right="200dp">
        <bitmap
            android:src="@drawable/stalogo_launcher"
            android:gravity="center" />
    </item>

</layer-list>